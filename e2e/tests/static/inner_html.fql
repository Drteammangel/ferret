LET doc = DOCUMENT(@url + '/static/blog.html')

LET expected = '<h4 class="font-italic">About</h4><p class="mb-0">Etiam porta <em>sem malesuada magna</em> mollis euismod. Cras mattis consectetur purus sit amet fermentum. Aenean lacinia bibendum nulla sed consectetur.</p>'
LET actual = INNER_HTML(doc, 'body > main > div > aside > div.p-3.mb-3.bg-light.rounded')

RETURN EXPECT(REGEXP_REPLACE(expected, '\s', ''), REGEXP_REPLACE(TRIM(actual), '(\n|\s)', ''))